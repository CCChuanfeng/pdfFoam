FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        system;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;
surfaceFormat raw;
interpolationScheme cellPoint;

fields
(
    Ux
    UCloudPDFx
    k
    kCloudPDF
    rho
    pndCloudPDF
    PaNIC
);

xstart 0.0;
xend   6;

n 100;

sets
(
    xaxis
    {
        type        uniform;
        axis        x;
        start       ($xstart 0.4 0.4);
        end         ($xend   0.4 0.4);
        nPoints     $n;
    }
);

surfaces
(
);

