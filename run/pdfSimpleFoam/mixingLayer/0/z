FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      z;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    lowerInlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    upperInlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
}


