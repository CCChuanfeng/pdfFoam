FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

evolutionFrequency 1;

cloudProperties
{
    averageTimeScale      1;
    particlesPerCell      50;
    particleNumberControl on;
    cloneAt               0.9;
    inoutPatches
    (
        lowerInlet
        upperInlet
        outlet
    );
    OmegaModel RAS;
    mixingModel IEM;
    reactionModel naiveFlamelet;
    scalarFields
    (
        z
    );
    mixedScalars
    (
        z
    );
    IEMCoeffs
    {
        Cmix 2.0;
    }
    naiveFlameletCoeffs
    {
        zName z;
    }
}

